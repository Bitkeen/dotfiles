[include]
    path = ~/.gitconfig.local
[alias]
    aa = add --all
    aip = add -i -p
    br = branch
    bl = blame
    co = checkout
    cob = checkout -b
    com = checkout master
    chp = cherry-pick
    ci = commit
    cia = commit --amend
    cim = commit -m
    cl = clone
    df = diff
    dfc = diff --check
    dfs = diff --staged
    dt = difftool
    lg = log --abbrev-commit
    lgg = log --all --oneline --graph
    pl = pull
    plrb = pull --rebase
    ps = push
    rb = rebase
    rs = reset
    sh = stash
    shl = stash list
    shp = stash pop
    st = status
    sw = show
[core]
    editor = vim
    whitespace = true
[diff]
    tool = vimdiff
[push]
	default = current
