[include]
    path = ~/.gitconfig.local
[alias]
    aip = add -i -p
    br = branch
    co = checkout
    cob = checkout -b
    ci = commit
    cl = clone
    df = diff
    dfc = diff --check
    dfs = diff --staged
    dt = difftool
    lg = log
    lgg = log --all --oneline --graph
    pl = pull
    plrb = pull --rebase
    ps = push
    rb = rebase
    sh = stash
    shl = stash list
    shp = stash pop
    st = status
[core]
    editor = vim
    whitespace = true
[diff]
    tool = vimdiff
